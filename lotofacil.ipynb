    "def is_even(n:int) -> bool:\n",
    "    '''\n",
    "    Returns True if n is even, False otherwise\n",
    "    '''\n",
    "    if n % 2 == 0:\n",
    "        return True\n",
    "    else:\n",
    "        return False\n",
    "\n",
    "def even_counter(game:list) -> int:\n",
    "    '''\n",
    "    Returns the number of even numbers in the game.\n",
    "    '''\n",
    "    even_count = 0\n",
    "    for ball in game:\n",
    "        if is_even(ball):\n",
    "            even_count += 1\n",
    "    return even_count"
   ]
