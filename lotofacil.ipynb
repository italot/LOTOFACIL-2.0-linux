    "def is_prime(n:int) -> bool:\n",
    "    '''\n",
    "    Returns True if n is prime, False otherwise.\n",
    "    '''\n",
    "    if n == 1:\n",
    "        return False\n",
    "    elif n == 2:\n",
    "        return True\n",
    "    else:\n",
    "        for x in range(2,n):\n",
    "            if n % x == 0:\n",
    "                return False\n",
    "        return True\n",
    "\n",
    "def prime_counter(game:list) -> int:\n",
    "    '''\n",
    "    Returns the number of prime numbers in the game.\n",
    "    '''\n",
    "    prime_count = 0\n",
    "    for ball in game:\n",
    "        if is_prime(ball):\n",
    "            prime_count += 1\n",
    "    return prime_count"
   ]
